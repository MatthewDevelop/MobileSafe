<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.foxconn.matthew.myapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name=".app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeNoTitle">
        <activity
            android:name=".activity.SplashActivity"
            android:theme="@style/fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".mobilesafe.activity.MobileSafeActivity" />
        <activity android:name=".mobilesafe.activity.SettingActivity" />
        <activity android:name=".mobilesafe.activity.LostFindActivity" />
        <activity android:name=".mobilesafe.activity.Setup1Activity" />
        <activity android:name=".mobilesafe.activity.Setup2Activity" />
        <activity android:name=".mobilesafe.activity.Setup3Activity" />
        <activity android:name=".mobilesafe.activity.Setup4Activity" />
        <activity android:name=".mobilesafe.activity.ContactActivity" />
        <activity android:name=".wanandroid.activity.WanAndroidActivity" />
        <activity android:name=".wanandroid.activity.WebViewActivity" />
        <activity android:name=".wanandroid.activity.LoginActivity" />
        <activity android:name=".wanandroid.activity.CollectActivity" />
        <activity android:name=".wanandroid.activity.SearchActivity" />

        <receiver
            android:name=".mobilesafe.receiver.BootCompleteReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <activity android:name=".websocketdemo.WebSocketActivity"></activity>
    </application>

</manifest>